<fieldset>
<legend><%= @dojo.branch %></legend>
<p>Branch Name: <%= @dojo.branch %></p>
<p>Street Address: <%= @dojo.address %></p>
<p>City: <%= @dojo.city %></p>
<p>State: <%= @dojo.state %></p>
</fieldset>

<fieldset>
  <legend>Students</legend>
<table>
  <thead>
    <tr>
      <th>First Name:</th>
      <th>Last Name:</th>
      <th>Email:</th>
      <th>Action:</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.email %></td>
      <td>
        <a href='/<%=@dojo.id%>/students/<%=student.id%>'>Show</a> |
        <a href='/<%=@dojo.id%>/students/<%=student.id%>/edit'>Edit</a> |
        <a href='/<%=@dojo.id%>/students/<%=student.id%>' data-method='delete'>Delete</a>
      </td>
    </tr>
      <% end %>
</table>
</fieldset>
<a href='/'> Home </a> || <a href='/<%=@dojo.id%>/students/new'>Add Student</a>
