<h3> Others who also added <%=@song.artist%> - <%=@song.title%> </h3>

<table>
  <thead>
    <tr>
      <th> Name</th>
      <th>Times Added</th>
    </tr>
  </thead>
  <tbody>
    <% @favorites.each do |x| %>

    <tr>
      <td>
        <a href='/users/<%=x.user.id%>'><%=x.user.first_name%> <%=x.user.last_name%></a>
      </td>
      <td><%=x.count%></td>
      <%end%>
    </tr>

  </tbody>
</table>
<br>
<a href='/songs'>Back to Dashboard</a>
